<% include nav.ejs %>
<div class="container">
                <h4>SignIn Here</h4>
                  <form action="/users/login" method="POST">
                <div class="row">
                  <div class="col s12 m4">
                      <a href="/auth/facebook" class="btn blue">facebook</a>
                        <a href="/auth/google" class="btn red">Google+</a>
                  </div>
                  <div class="col s12 m6">
                      <div class="input-field">
                          <i class="material-icons prefix black-text">person</i>
                           <input type="text" name="username" placeholder="Username">
                    </div>
                    <div class="input-field">
                          <div class="input-field">
                                  <i class="material-icons prefix black-text">lock</i>
                                   <input type="password" name="password" id="password" placeholder="*******">
                          </div>
                        </div>
                        <input type="submit" class="btn green right btn-small" value="Sign In">
               
                  </div>
                </div>
              
              </form>
              </div>